{
  "name": "token-types",
  "version": "1.0.1",
  "description": "Common token types for decoding and encoding binairy values",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "scripts": {
    "build": "npm run compile",
    "compile-src": "tsc -p src",
    "compile-test": "tsc -p test",
    "compile": "npm run compile-src && npm run compile-test",
    "prepare": "npm run compile",
    "lint-ts": "tslint 'src/**/*.ts' --exclude 'src/**/*.d.ts' 'test/**/*.ts' --exclude 'test/**/*.d.ts'",
    "lint-md": "remark -u preset-lint-recommended .",
    "lint": "npm run lint-ts && npm run lint-md",
    "test": "mocha --require ts-node/register test",
    "cover-test": "nyc npm run test",
    "coveralls": "npm run cover-test && nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=0.1.98"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Borewit/token-types"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "lib/index",
  "bugs": {
    "url": "https://github.com/Borewit/token-types/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.7.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {},
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended"
    ]
  }
}
